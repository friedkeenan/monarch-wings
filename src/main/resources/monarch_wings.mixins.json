{
    "required": true,
    "minVersion": "0.8",
    "package": "io.github.friedkeenan.monarch_wings.mixin",
    "compatibilityLevel": "JAVA_17",

    "mixins": [
        "DisableGliding",
        "LocalDisableDoubleJumpWhenFlying",
        "ServerDisableDoubleJumpWhenFlying",
        "ClientboundAbilitiesDisableDoubleJumpWhenFlying",
        "ServerboundAbilitiesDisableDoubleJumpWhenFlying",
        "AllowDoubleJump",
        "PairDoubleJumpEnabled",
        "FlapElytraModel"
    ],

    "injectors": {
        "defaultRequire": 1
    }
}
